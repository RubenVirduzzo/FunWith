<section class="index-section">
      <%= render "nav_events" %>
      <%= render "search_form" %>
    
      <% if @events.present? %>
        <div class="container container-events">
          <%= render "events" %>
        </div>
      <% else %>
        <h6>
        There are no events yet, add one!
        <h6>
        <%= link_to "New event", new_event_path %>
      <% end %>
</section>